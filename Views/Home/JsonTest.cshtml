
<div class="text-center">
    <h1 class="display-4">JsonTest</h1>
</div>

<div>
    <ul id="ul1"></ul>
</div>




@section Scripts {

    <script>

        const emps = [{ "name": "Tom", "workYears": 3, "salary": 35000 },
        { "name": "Jack", "workYears": 5, "salary": 40000 },
        { "name": "Mary", "workYears": 7, "salary": 45000 }];

        const ul1 = document.getElementById('ul1');

        const liEmps = emps.map(emp => `<li>name: ${emp.name} workYears: ${emp.workYears} salary: ${emp.salary}</li>`).join('');

        ul1.innerHTML = liEmps;


    </script>
}